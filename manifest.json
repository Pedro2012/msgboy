{
  "name": "Msgboy",
  "version": "0.0.14",
  "description": "Grownme makes Chrome more useful and smarter!",
  "icons": {
    "24": "/views/icons/24.png",
    "128": "/views/icons/128.png"
  },
  "options_page": "/views/html/options.html",
  "app": {
    "launch": {
      "local_path": "/views/html/inbox.html"
    }
  },
  "permissions": [
    "notifications",
    "tabs",
    "background",
    "management",
    "unlimitedStorage",
    "http://*/",
    "https://*/"
  ],
  "content_scripts": [
    {
      "js": [
        "/lib/date.format.js",
        "/lib/jquery/jquery.js",
        "/lib/jquery/jquery.event.drag.js",
        "/controllers/utils.js",
        "/controllers/feed_sniffer.js",
        "/controllers/show_bookmark.js"
      ],
      "css": [
        "/views/css/bookmark.css"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    },
    {
      "js": [
        "/lib/date.format.js",
        "/lib/jquery/jquery.js",
        "/lib/jquery/jquery.event.drag.js",
        "/controllers/utils.js",
        "/controllers/site_plugins.js",
        "/controllers/plugins/tumblr.js",
        "/controllers/plugins/posterous.js",
        "/controllers/plugins/google-reader.js",
        "/controllers/plugins/digg.js",
        "/controllers/plugins/github-repos.js",
        "/controllers/plugins/github-users.js",
        "/controllers/plugins/disqus.js",
        "/controllers/plugins/statusnet.js",
        "/controllers/plugins/typepad.js",
        "/controllers/plugins/quora-people.js",
        "/controllers/plugins/quora-topics.js",
        "/controllers/run_plugins.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "background_page": "/views/html/background.html",
  "update_url": "http://sup.ee/update-msgboy"
}